
uint8_t Led1 = D3;

void setup(){
  pinMode (Led1, OUTPUT);
}



void loop() {
  digitalWrite(Led1, HIGH);
  delay(2000);
  digitalWrite(Led1, LOW);
  delay(500);
 
  digitalWrite(Led1, HIGH);
  delay(1000);
  digitalWrite(Led1, LOW);
  delay(500);
 
  digitalWrite(Led1, HIGH);
  delay(1000);
  digitalWrite(Led1, LOW); 
  delay(2000);
 
  digitalWrite(Led1, HIGH);
  delay(1000);
  digitalWrite(Led1, LOW);
  delay(500);
 
  digitalWrite(Led1, HIGH);
  delay(2000);
  digitalWrite(Led1, LOW);
  delay(500);
 
  digitalWrite(Led1, HIGH);
  delay(2000);
  digitalWrite(Led1, LOW);
  delay(500);
  
  digitalWrite(Led1, HIGH);
  delay(1000);
  digitalWrite(Led1, LOW);
  delay(500);
  
  
  
}
